{
  "name": "first-project",
  "scripts": {
    "postinstall": "npm run shared:build && PRISMA_ENGINES_CHECKSUM_IGNORE_MISSING=1 prisma generate && tsr generate",
    "setup": "npm run shared:build && PRISMA_ENGINES_CHECKSUM_IGNORE_MISSING=1 prisma generate && tsr generate",
    "dev": "vinxi dev",
    "build": "npm run shared:build && vinxi build",
    "start": "vinxi start",
    "preview": "node server.cjs",
    "typecheck": "npm run shared:build && tsc --noEmit",
    "lint": "eslint . -c eslint.config.mjs --max-warnings 0",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:schemas": "vitest --run src/test/schemas",
    "db:generate": "prisma migrate dev",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "format": "prettier --write .",
    "shared:build": "cd packages/shared && npm run build",
    "shared:dev": "cd packages/shared && npm run dev"
  },
